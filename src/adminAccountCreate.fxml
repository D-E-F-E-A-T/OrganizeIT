<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adminAccountCreateController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Create An Account">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="52.0" text="Fill up all the details to create an admin Account..." />
      <Label layoutX="39.0" layoutY="103.0" text="Admin's First Name : " />
      <TextField fx:id="adminFirstNameField" layoutX="226.0" layoutY="98.0" />
      <Label layoutX="39.0" layoutY="146.0" text="Admin's Second Name : " />
      <TextField fx:id="adminSecondNameField" layoutX="226.0" layoutY="141.0" />
      <Label layoutX="39.0" layoutY="192.0" text="Username : " />
      <TextField fx:id="adminUsernameField" layoutX="226.0" layoutY="187.0" />
      <PasswordField fx:id="adminPasswordField" layoutX="226.0" layoutY="230.0" />
      <Label layoutX="39.0" layoutY="235.0" text="Password : " />
      <Button fx:id="continueButton" layoutX="262.0" layoutY="338.0" mnemonicParsing="false" text="Continue" />
      <DialogPane fx:id="errorDialogPane" layoutX="82.0" layoutY="83.0" prefHeight="264.0" prefWidth="431.0">
         <header>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label layoutX="14.0" layoutY="14.0" text="Uh Oh!">
                     <font>
                        <Font size="29.0" />
                     </font>
                  </Label>
                  <Label layoutX="14.0" layoutY="53.0" text="Please check the following errors and try again." />
                  <Label fx:id="errorLabelDialog" alignment="TOP_LEFT" layoutX="14.0" layoutY="78.0" prefHeight="75.0" prefWidth="316.0" text="Errors will come here!" wrapText="true" />
               </children>
            </AnchorPane>
         </header>
      </DialogPane>
   </children>
</AnchorPane>
